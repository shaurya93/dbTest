<script>
$(document).ready(function(){
 $(window).resize(function(){
 if(($(window).width())<767)
{  
  $("ul.nav li").removeClass("drop");
  $("ul.nav li").addClass("dropdown");
  $("ul.nav li.dropdown ul").removeClass("nav");
  $("ul.nav li.dropdown ul").addClass("dropdown-menu");
}
else{ 
  $("ul.nav li").removeClass("dropdown");
  $("ul.nav li").addClass("drop");
  $("ul.nav li.drop ul").removeClass("dropdown-menu");
  $("ul.nav li.drop ul").addClass("nav");
}
});
$('input').blur(function(){
if($(this).val())
$(this).addClass('used');
else
$(this).removeClass('used');
});
 $('#noti_Counter')
            .css({ opacity: 0 })
            .text(<%= count_rend%>)              // ADD DYNAMIC VALUE (YOU CAN EXTRACT DATA FROM DATABASE OR XML).
            .css({ top: '-10px' })
            .animate({ top: '-2px', opacity: 1 }, 500);

        $('#noti_Button').click(function () {

            // TOGGLE (SHOW OR HIDE) NOTIFICATION WINDOW.
            $('#notifications').fadeToggle('fast', 'linear', function () {
                if ($('#notifications').is(':hidden')) {
                    $('#noti_Button').css('background-color', '#2E467C');
                }
                else $('#noti_Button').css('background-color', '#FFF');        // CHANGE BACKGROUND COLOR OF THE BUTTON.
            });

            $('#noti_Counter').fadeOut('slow');                 // HIDE THE COUNTER.

            return false;
        });

        // HIDE NOTIFICATIONS WHEN CLICKED ANYWHERE ON THE PAGE.
        $(document).click(function () {
            $('#notifications').hide();

            // CHECK IF NOTIFICATION COUNTER IS HIDDEN.
            if ($('#noti_Counter').is(':hidden')) {
                // CHANGE BACKGROUND COLOR OF THE BUTTON.
                $('#noti_Button').css('background-color', '#2E467C');
            }
        });

        $('#notifications').click(function () {
            return false;       // DO NOTHING WHEN CONTAINER IS CLICKED.
        });
});
$("#myBtn").click(function(){
        $("#myModal").modal();
    });
</script>
