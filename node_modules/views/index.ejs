<style>
form { padding: 2.5em; }
.group{ position: relative; margin-bottom: 2.4em;}
label{
color: #d2d2d2;
font-size: 15px;
font-weight: normal;
font-family: sans-serif;
position: absolute;
pointer-events: none;
top: 10px;
transition: 0.2s ease all;
}
input{
color: #909090;
font-size: 15px;
padding: 10px 0px 10px 0px;
display: block;
outline: none;
border: none;
width: 300px;
border-bottom: 30px solid #d2d2d2;
}
.bar {
position: relative; display: block; width: 300px;}
.bar:before, .bar:after{
content: "";
height: 2px;
width: 0;
bottom: -1px;
position: absolute;
background: #B388FF;
transition: 0.2s ease all;
}
.bar:before{left: 50%;}
.bar:after{right: 50%;}
input:valid { border-bottom: 1px solid #4fc24f;}
input-valid ~ .bar:before,
input-valid ~ .bar:after {
background-color: #4fc24f !important;
}
input:focus ~ label,
input.used ~ label {
top: -20px;
font-size: 13px;
color: #B388FF;
}
input:valid ~ label,
input:valid.used ~label {
color: #4fc24f;
}
input:focus ~ .bar:before,
input:focus ~ .bar:after{
width: 50%;
}
</style>


